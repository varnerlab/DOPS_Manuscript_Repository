	Pmid = {21943356},
	Pst = {ppublish},
	Title = {Data-driven modelling of receptor tyrosine kinase signalling networks quantifies receptor-specific potencies of PI3K- and Ras-dependent ERK activation},
	Volume = {441},
	Year = {2012},
