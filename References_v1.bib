	Doi = {10.1016/j.thromres.2014.03.005},
	Journal = {Thromb Res},
	Journal-Full = {Thrombosis research},
	Keywords = {Coagulation; Mathematical model; Multiscale model; Thrombus formation; Thrombus permeability},
